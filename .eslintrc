{
  "extends": "airbnb",
  "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
  "rules": {
    "no-console": 0,
    "max-len": ["error", 200],
    "react/no-string-refs": 0,
    "dot-notation": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0,
    "no-undef": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "react/no-array-index-key": 0
  }
}
